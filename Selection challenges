rain = str(input("Is it raining today?(y/n)")).lower()
if rain == "y":
    wind = str(input("Is it windy today?(y/n)")).lower()
    if wind == "y":
        print("It is too windy for an umbrella lah")
    else:
        print("Remember to take an umbrella")
else:
    print("Enjoy your day")

