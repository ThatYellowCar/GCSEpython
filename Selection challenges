number = int(input("Give me a random ahh number between 10 and 20: "))
if number > 10 and number < 20:
    print("Nice one!")
elif number == 10:
    print("I said between buddy try again")
    number = int(input("Give me another random ahh number between 10 and 20: "))
    if 10 < number > 20:
        print("Nice second try!")
    elif number == 20:
        print("You got it wrong but i dont really care anymore")
    elif number == 10:
        print("You got it wrong but i dont really care anymore")
    else:
        print("I give up how can you get it wrong 2 times in a row")

elif number == 20:
    print("I said between buddy try again")
    number = int(input("Give me another random ahh number between 10 and 20: "))
    if 10 < number > 20:
        print("Nice second try!")
    elif number == 20:
        print("You got it wrong but i dont really care anymore")
    elif number == 10:
        print("You got it wrong but i dont really care anymore")
    else:
        print("I give up how can you get it wrong 2 times in a row")
else:
    print("Bruh are you stupid?")
             
