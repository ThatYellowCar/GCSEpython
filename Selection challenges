number1 = int(input("Give me a random ahh number BELOW 20: "))
if number1 < 20:
    print("nice one")
elif number1 == 20:
    print("You didnt read the question properly but I'll let it slide")
else:
    print("HOW DARE YOU DISOBEY ME! YOU WILL BE SENTENCED TO LIFE IN PRISON!")

